# Techtonica Dedicated Server Configuration
# Copy this file to .env and fill in values

# Steam Credentials (REQUIRED)
# Must be an account that owns Techtonica
# First login requires Steam Guard 2FA - run interactively first
STEAM_USER=your_steam_username
STEAM_PASS=your_steam_password

# Game Settings
SAVE_NAME=server_world
PUBLIC_LOBBY=0

# Performance
SKIP_UPDATE=0
